{
    "name": "FBMeet User Hacking",
	"description": "Facebook Hackathon!",
    "version": "4.5",
    "manifest_version": 4,
	"permissions": [
		"tabs"
	],
	"background": {
		"scripts": ["event_manager_bs.js"]
	},
	"content_scripts": [
		{
		    "matches": ["*://www.facebook.com/*"],
			"js": ["jquery.js", "fbMeet.js", "event_caller_cs.js"]
		}, {
	        "matches": ["*://www.facebook.com/events/list/"],
	        "run_at": "document_start",
	        "js": ["jquery.js", "user_sim_cs.js"]
	    }, {
	    	"matches": ["*://www.facebook.com/events/*"],
	    	"exclude_matches": ["*://www.facebook.com/events/list/"],
	    	"js": ["invite_user_sim_cs.js"]
	    }
	]
}